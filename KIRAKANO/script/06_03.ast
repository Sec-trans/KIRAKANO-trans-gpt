block_00210={
{"bg",sync=0,face=1,file="zev_com_c08_02",time=400,path=":ev/ev_com_c08/",set="ev_com_c08",ax=1280,ay=720,bx=-640,by=-360,x=400,y=230},
{"text"},
text={
