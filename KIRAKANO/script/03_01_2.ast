{"fg",ch="月姫",size="no",mode=2,file="run_2",path=":fg/",face="穏やかな笑み",always="おさんぽ",lv=3,id=15},
{"fgact",ch="月姫",act="ありがとう",lv=3,id=15,emote=1},
{"text"},
