vo={
{"vo",file="fem_kir_00026",ch="kir"},
},
ja={
{
name={"琪拉拉"},
"「哇……！」",
{"rt2"},
},
},
},
linkback="block_00072",
linknext="block_00074",
},
block_00074={
{"text"},
