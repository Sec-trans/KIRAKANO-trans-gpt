vo={
{"vo",file="fem_kir_00022",ch="kir"},
},
ja={
{
name={"琪拉拉"},
"「…………」",
{"rt2"},
},
},
},
linkback="block_00056",
linknext="block_00058",
},
block_00058={
{"text"},
text={
ja={
{
name={"让"},
"「……琪拉拉，没问题吧？」",
{"rt2"},
},
},
},
linkback="block_00057",
linknext="block_00059",
},
